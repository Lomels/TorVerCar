<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Rectangle?>

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity"
	prefHeight="400.0" prefWidth="600.0"
	xmlns="http://javafx.com/javafx/8.0.171"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="logic.view.booking.LiftListView">
	<children>
		<Rectangle arcHeight="5.0" arcWidth="5.0" fill="#046104"
			height="40.0" layoutX="5.0" stroke="#046104" strokeType="INSIDE"
			width="600.0" AnchorPane.bottomAnchor="360.0"
			AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
			AnchorPane.topAnchor="0.0" />
		<Button fx:id="btHome" layoutX="24.0" layoutY="7.0"
			mnemonicParsing="false" onAction="#homeButtonController"
			style="-fx-background-color: #046104;" text="Home" />
		<Button fx:id="btBook" layoutX="90.0" layoutY="7.0"
			mnemonicParsing="false" onAction="#bookButtonController"
			style="-fx-background-color: green;" text="Book" underline="true" />
		<Button fx:id="btOffer" layoutX="148.0" layoutY="7.0"
			mnemonicParsing="false" onAction="#offerButtonController"
			style="-fx-background-color: #046104;" text="Offer" />
		<Button fx:id="btLogout" layoutX="558.0" layoutY="7.0"
			maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
			minWidth="-Infinity" mnemonicParsing="false"
			onAction="#logoutButtonController" prefHeight="26.0" prefWidth="43.0"
			style="-fx-background-color: #046104;">
			<graphic>
				<ImageView fitHeight="29.0" fitWidth="33.0">
					<image>
						<Image url="@../../../../Scaricati/logout.png" />
					</image>
				</ImageView>
			</graphic>
		</Button>
		<Button fx:id="btProfile" layoutX="512.0" layoutY="7.0"
			minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false"
			onAction="#profileButtonController" prefHeight="26.0"
			prefWidth="46.0" style="-fx-background-color: #046104;">
			<graphic>
				<ImageView fitHeight="30.0" fitWidth="28.0">
					<image>
						<Image url="@../../../../Scaricati/user.png" />
					</image>
				</ImageView>
			</graphic>
		</Button>
		<Button fx:id="btMyCar" layoutX="206.0" layoutY="7.0"
			mnemonicParsing="false" onAction="#myCarButtonController"
			style="-fx-background-color: #046104;" text="My Car " />
		<ComboBox fx:id="btMyLift" layoutX="274.0" layoutY="6.0"
			minHeight="-Infinity" minWidth="-Infinity" prefHeight="28.0"
			prefWidth="103.0" promptText="My Lift"
			style="-fx-background-color: #046104; -fx-border-color: #046104;" />
		<ScrollPane layoutX="140.0" layoutY="114.0"
			prefHeight="200.0" prefWidth="459.0" AnchorPane.bottomAnchor="0.0"
			AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
			AnchorPane.topAnchor="40.0">
			<content>
				<VBox prefHeight="360.0" prefWidth="600.0">
					<children>
						<ListView fx:id="liftList" maxHeight="-Infinity"
							maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
							prefHeight="353.0" prefWidth="580.0" />
						<Button fx:id="btConfirm" mnemonicParsing="false"
							onAction="#confirmButtonController" text="Confirm"
							translateX="500.0" />
					</children>
				</VBox>
			</content>
		</ScrollPane>
	</children>
</AnchorPane>
