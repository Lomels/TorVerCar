<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.effect.InnerShadow?>
<?import javafx.scene.effect.MotionBlur?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.paint.LinearGradient?>
<?import javafx.scene.paint.Stop?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.layout.HBox?>

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity"
	prefHeight="514.0" prefWidth="771.0"
	xmlns="http://javafx.com/javafx/8.0.171"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="logic.view.booking.BookView"
	stylesheets="@../fxml/TorVerCar.css">
	<children>
		<HBox layoutX="-9.0" layoutY="-9.0" AnchorPane.leftAnchor="0.0"
			AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
			<children>
				<ButtonBar nodeOrientation="RIGHT_TO_LEFT"
					prefHeight="46.0" prefWidth="771.0"
					style="-fx-background-color: #39393a;">
					<buttons>
						<Button fx:id="btMyCar" mnemonicParsing="false"
							onAction="#myCarButtonController" styleClass="button-unactive"
							text="MyCAR">
							<font>
								<Font size="18.0" />
							</font>
						</Button>
						<Button fx:id="btOffer" mnemonicParsing="false"
							onAction="#offerButtonController" styleClass="button-unactive"
							 text="OFFER">
							<font>
								<Font size="18.0" />
							</font>
						</Button>
						<Button fx:id="btBook" mnemonicParsing="false"
							onAction="#bookButtonController" styleClass="button-active"
							text="BOOK" prefHeight="46.0" prefWidth="83.0">
							<font>
								<Font size="18.0" />
							</font>
						</Button>
						<Button fx:id="btHome" mnemonicParsing="false"
							onAction="#homeButtonController" styleClass="button-unactive"
							text="HOME" textAlignment="CENTER" textOverrun="CLIP"
							wrapText="true">
							<font>
								<Font size="18.0" />
							</font>
						</Button>
					</buttons>
					<effect>
						<DropShadow radius="4.75" width="0.0" />
					</effect>
				</ButtonBar>
				<ButtonBar nodeOrientation="LEFT_TO_RIGHT"
					prefHeight="46.0" prefWidth="771.0"
					style="-fx-background-color: #39393a;">
					<buttons>
						<Button fx:id="btLogout" maxHeight="-Infinity" 
							maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
							mnemonicParsing="false" onAction="#logoutButtonController"
							prefHeight="26.0" prefWidth="43.0" styleClass="button-unactive">
							<graphic>
								<ImageView fitHeight="29.0" fitWidth="33.0">
									<image>
										<Image url="@../../../../images/logout.png" />
									</image>
								</ImageView>
							</graphic>
						</Button>
						<Button fx:id="btProfile" minHeight="-Infinity"
							minWidth="-Infinity" mnemonicParsing="false"
							onAction="#profileButtonController" prefHeight="26.0"
							prefWidth="46.0" styleClass="button-unactive" text="Profile">
							<graphic>
								<ImageView fitHeight="30.0" fitWidth="28.0">
									<image>
										<Image url="@../../../../images/user.png" />
									</image>
								</ImageView>
							</graphic>
						</Button>
					</buttons>
					<effect>
						<DropShadow radius="4.75" width="0.0" />
					</effect>
				</ButtonBar>
			</children>
		</HBox>
		<ComboBox fx:id="btMyLift" layoutX="273.0" layoutY="6.0"
			minHeight="-Infinity" minWidth="-Infinity" prefHeight="28.0"
			prefWidth="103.0" promptText="My Lift" visible="false"
			style="-fx-background-color: #046104; -fx-border-color: #046104;" />
		<Text layoutX="22.0" layoutY="165.0" strokeType="OUTSIDE"
			strokeWidth="0.0" text="Starting point:" />
		<Text layoutX="22.0" layoutY="204.0" strokeType="OUTSIDE"
			strokeWidth="0.0" text="Destination:" />
		<Text layoutX="20.0" layoutY="300.0" strokeType="OUTSIDE"
			strokeWidth="0.0" text="Depart from:" />
		<Text layoutX="20.0" layoutY="335.0" strokeType="OUTSIDE"
			strokeWidth="0.0" text="Arrive up:" />
		<TextField fx:id="tfStartPoint" layoutX="262.0"
			layoutY="147.0" promptText="Street, CAP, Place" />
		<TextField fx:id="tfArrivalPoint" layoutX="262.0"
			layoutY="187.0" promptText="Street, CAP, Place" />
		<TextField fx:id="tfStartTime" layoutX="262.0"
			layoutY="282.0" promptText="HH:MM" />
		<TextField fx:id="tfArrivalTime" layoutX="262.0"
			layoutY="317.0" promptText="HH:MM" />
		<Button fx:id="btFind" layoutX="511.0" layoutY="352.0" styleClass="button-active"
			mnemonicParsing="false" onAction="#findButtonController"
			text="Find Lift" />
		<Button fx:id="btCheckStart" layoutX="439.0" layoutY="147.0" styleClass="button-active"
			mnemonicParsing="false" onAction="#startButtonController" text="Find" />
		<Button fx:id="btCheckEnd" layoutX="439.0" layoutY="187.0" styleClass="button-active"
			mnemonicParsing="false" onAction="#destButtonController" text="Find" />
		<Text layoutX="23.0" layoutY="262.0" strokeType="OUTSIDE"
			strokeWidth="0.0" text="Day:" />
		<CheckBox fx:id="cbGoing" layoutX="205.0" layoutY="76.0"
			mnemonicParsing="false" text="Going" />
		<CheckBox fx:id="cbReturn" layoutX="332.0" layoutY="76.0"
			mnemonicParsing="false" text="Return" />
		<DatePicker fx:id="dpDate" layoutX="262.0" layoutY="252.0" />
	</children>
</AnchorPane>
